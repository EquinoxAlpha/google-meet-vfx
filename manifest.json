{
    "manifest_version": 2,
    "name": "Google Meet Visual Effects",
    "version": "1.1",
    "description": "Adds some extra options to Google Meet.",
    "browser_specific_settings": {
        "gecko": {
            "id": "g_meet_vfx@siegebyte.xyz"
        }
    },
    "content_scripts": [
        {
            "run_at": "document_start",
            "matches": [
                "*://meet.google.com/*"
            ],
            "js": [
                "injector.js"
            ]
        }
    ]
}
